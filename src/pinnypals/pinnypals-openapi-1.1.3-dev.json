  "paths": {
    "/events/{id}": {
      "get": {
        "tags": [
          "Item Data"
        ],
        "description": "Get an event by id",
        "parameters": [
          {
            "in": "path",
            "name": "id",
            "required": true,
            "schema": {
              "type": "integer",
              "minimum": 0
            },
            "description": "The event id"
          }
        ],
        "responses": {
          "200": {
            "description": "The event",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/EventDto"
                }
              }
            }
          },
          "404": {
            "description": "Event not found"
          }
        }
      }
    },
    "/groups/{id}/pins": {
      "get": {
        "tags": [
          "Item Data"
        ],
        "description": "Get all pins for a pin group",
        "parameters": [
          {
            "in": "path",
            "name": "id",
            "required": true,
            "schema": {
              "type": "integer",
              "minimum": 0
            },
            "description": "The pin group id"
          }
        ],
        "responses": {
          "200": {
            "description": "The pins in the pin group",
            "content": {
              "application/json": {
                "schema": {
                  "type": "array",
                  "items": {
                    "$ref": "#/components/schemas/PinDto"
                  }
                }
              }
            }
          },
          "404": {
            "description": "Pin group not found"
          }
        }
      }
    },
    "/groups/{id}": {
      "get": {
        "tags": [
          "Item Data"
        ],
        "description": "Get a pin group by id",
        "parameters": [
          {
            "in": "path",
            "name": "id",
            "required": true,
            "schema": {
              "type": "integer",
              "minimum": 0
            },
            "description": "The pin group id"
          }
        ],
        "responses": {
          "200": {
            "description": "The pin group",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/PinGroupDto"
                }
              }
            }
          },
          "404": {
            "description": "Pin group not found"
          }
        }
      }
    },
    "/item-data": {
      "get": {
        "tags": [
          "Item Data"
        ],
        "description": "Get all item data",
        "responses": {
          "200": {
            "description": "The item data",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ItemDataDto"
                }
              }
            }
          }
        }
      }
    },
    "/pins/{id}": {
      "get": {
        "tags": [
          "Item Data"
        ],
        "description": "Get a pin by id",
        "parameters": [
          {
            "in": "path",
            "name": "id",
            "required": true,
            "schema": {
              "type": "integer",
              "minimum": 0
            },
            "description": "The pin id"
          }
        ],
        "responses": {
          "200": {
            "description": "The pin",
