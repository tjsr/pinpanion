{
  "name": "pinnysets",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "dependencies": {
    "@emotion/react": "^11.10.4",
    "@emotion/styled": "^11.10.4",
    "@mui/icons-material": "^5.10.6",
    "@mui/material": "^5.10.6",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/express": "^4.17.14",
    "@types/jest": "^27.5.2",
    "@types/node": "^16.11.60",
    "@types/react": "^18.0.21",
    "@types/react-dom": "^18.0.6",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^8.5.0",
    "express": "^4.18.1",
    "fuzzy": "^0.1.3",
    "node-fetch": "^3.2.10",
    "prettier": "^2.7.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-qr-code": "^2.0.8",
    "react-scripts": "5.0.1",
    "react-virtualized-auto-sizer": "^1.0.7",
    "react-window": "^1.8.7",
    "typescript": "^4.8.3",
    "use-hash-param": "^0.2.4",
    "uuid": "^9.0.0",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start:proxy": "node --loader ts-node/esm src/proxy.ts",
    "start": "react-scripts start",
    "build": "npm run download -- public/imgs && react-scripts build",
    "build:amplify": "npm run download:amplify && react-scripts build",
    "download": "node --experimental-specifier-resolution=node --loader ts-node/esm ./src/utils/pindownload.ts",
    "download:test": "TEST_MODE=true node --experimental-specifier-resolution=node --loader ts-node/esm ./src/utils/pindownload.ts",
    "download:amplify": "IMAGES_CACHE_DIR=images node --experimental-specifier-resolution=node --loader ts-node/esm ./src/utils/pindownload.ts",
    "test:react": "react-scripts test",
    "test": "npx jest",
    "lint": "eslint --fix \"./src/**/*.{ts,tsx}\"",
    "prettier": "prettier --single-quote --write \"./src/**/*.{ts,tsx}\"",
    "format": "npm run prettier && npm run lint",
    "ios": "node node_modules/react-native/local-cli/cli.js run-ios",
    "android": "node node_modules/react-native/local-cli/cli.js run-android"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/react-virtualized-auto-sizer": "^1.0.1",
    "@types/react-window": "^1.8.5",
    "@types/uuid": "^8.3.4",
    "jest": "^29.0.3",
    "ts-jest": "^29.0.2",
    "ts-node": "^10.9.1"
  }
}
